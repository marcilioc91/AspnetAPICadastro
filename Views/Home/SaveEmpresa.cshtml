@{
    ViewBag.Title = "Home Page";
}

<br>

<div class="alert alert-success">
 Dados Salvos com Sucesso!
</div>
<br>

<div class="col-lg-12">
    <table class="table table-responsive">
        <thead>
            <tr>
                <th>Id</th>
                <th>CNPJ</th>
                <th>Telefone</th>
                <th>Email</th>
                <th>Data e Hora do Registro</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var empresa in ViewBag.empresas)
            {
            <tr>
                <td>@empresa.id</td>
                <td>@empresa.cnpj</td>
                <td>@empresa.telefone</td>
                <td>@empresa.email</td>
                <td>@empresa.data_do_cadastro</td>
            </tr>
             }
        </tbody>
    </table>
</div>